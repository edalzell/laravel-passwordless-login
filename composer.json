{
    "name": "grosv/laravel-passwordless-login",
    "description": "Passwordless login using temporary signed URL",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ed Grosvenor",
            "email": "ed.grosvenor@wickedviral.com"
        },
        {
            "name": "Erin Dalzell",
            "email": "erin@silentz.co"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.3",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Grosv\\LaravelPasswordlessLogin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "App\\": "vendor/orchestra/testbench-core/laravel/app"
        }
    },
    "extra": {
        "laravel": {
            "providers": "Grosv\\LaravelPasswordlessLogin\\LaravelPasswordlessLoginProvider"
        }
    }
}
